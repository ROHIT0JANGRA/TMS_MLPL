@model CodeLock.Models.MasterProductTemperature
@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="widget-wrap">
    <div class="row">
        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.ProductId)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.ProductName)
                </dd>
            </dl>
        </div>
        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.PreLoadedTemperature)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.PreLoadedTemperature)
                </dd>
            </dl>
        </div>

        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.MinimumTemperature)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.MinimumTemperature)
                </dd>
            </dl>
        </div>
        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.MaximumTemperature)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.MaximumTemperature)
                </dd>
            </dl>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.StandardTemperature)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.StandardTemperature)
                </dd>
            </dl>
        </div>
        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.MinimumHumidity)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.MinimumHumidity)
                </dd>
            </dl>
        </div>
        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.MaximumHumidity)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.MaximumHumidity)
                </dd>
            </dl>
        </div>
        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.StandardHumidity)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.StandardHumidity)
                </dd>
            </dl>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.IsActive)
                </dt>
                <dd>
                    @Html.YesNo(Model.IsActive)
                </dd>
            </dl>
        </div>
        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.EntryByName)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.EntryByName)
                </dd>
            </dl>
        </div>
        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.EntryDate)
                </dt>
                <dd>
                    @Model.EntryDate.ToDisplayDateTime()
                </dd>
            </dl>
        </div>
        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.UpdateByName)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.UpdateByName)
                </dd>
            </dl>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-3">
            <dl>
                <dt>
                    @Html.DisplayNameFor(model => model.UpdateDate)
                </dt>
                <dd>
                    @Model.UpdateDate.ToDisplayDateTime()
                </dd>
            </dl>
        </div>
    </div>
</div>
</div>
<script>
    $(document).ready(function () {
        SetPageLoad('Product Temperature', 'View Detail', '', 'Goto List', '@Url.Action("Index")', 'Add New', '@Url.Action("Insert")', 'Update', '@Url.Action("Update", new { id = Model.ProductId })');
    });
</script>

