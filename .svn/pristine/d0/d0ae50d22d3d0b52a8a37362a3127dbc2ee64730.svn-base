        function SumLoadDataTable(obj, col) {

            $(obj).dataTable({
                'bSort': false,
                'bFilter': true,
                'bPaginate': (true),//Paging
                "footerCallback": function (tfoot, data, start, end, display) {
                    var api = this.api(), data;
                    var currentPagetotal = api.column(col, { page: 'current' }).data().reduce(function (a, b) {
                        return parseFloat(a) + parseFloat(b);
                    });

                    var total = api.column(col).data().reduce(function (a, b) {
                        return parseFloat(a) + parseFloat(b);
                    });

                    $(tfoot).find('th').eq(1).html(currentPagetotal.toFixed(2) + " of " + total.toFixed(2));
                }
            });
        }



