using System;

namespace CodeLock.Models
{
  public class OilExpenses
  {
    public OilExpenses()
    {
      this.PetrolPumpId = (short) 0;
      this.PetrolPumpName = "";
      this.Km = 0;
      this.BillNo = "";
      this.BillDate = DateTime.Now;
      this.FuelQuantity = new Decimal(0);
      this.FuelRate = new Decimal(0);
      this.Amount = new Decimal?(new Decimal(0));
      this.ApprovedAmount = new Decimal(0);
      this.PaidBy = (byte) 0;
      this.Remarks = "";
    }

    public long TripsheetId { get; set; }

    public short PetrolPumpId { get; set; }

    public string PetrolPumpName { get; set; }

    public int Km { get; set; }

    public string BillNo { get; set; }

    public DateTime BillDate { get; set; }

    public Decimal FuelQuantity { get; set; }

    public Decimal FuelRate { get; set; }

    public Decimal? Amount { get; set; }

    public Decimal ApprovedAmount { get; set; }

    public byte PaidBy { get; set; }

    public short? CardId { get; set; }

    public string Remarks { get; set; }
  }
}
