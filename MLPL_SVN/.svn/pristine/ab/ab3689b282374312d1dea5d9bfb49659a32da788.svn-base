@using CodeLock.Models
@model Grn
@{
    ViewBag.Title = "InsertFromAsn";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script>
    baseUrl = '@Url.Action("", "Asn" ,new { Area = "WMS" })';
    companyId = '@SessionUtility.CompanyId';
    warehouseId = '@SessionUtility.WarehouseId';
</script>
<script src="@Url.Content("~/Areas/WMS/Views/Grn/InsertFromAsn.js")"></script>
<div id="dvWizard">
    <div class="panel-body">
        @using (Html.BeginForm("InsertFromAsn", "Grn", FormMethod.Post, new { @class = "j-forms", @id = "j-forms" }))
        {
            @Html.AntiForgeryToken()
            @Html.HiddenFor(m => m.GrnId, new { @id = "hdnGrnId" })
            @Html.HiddenFor(m => m.AsnId, new { @id = "hdnAsnId" })
            <div class="tab-content">
                <div class="tab-pane">
                    <div class="row">
                        <div class="col-sm-3">
                            @Html.Label("ASN Date", new { @class = "label" })
                            <div id="drAsnDate"></div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                @Html.Label("Supplier Type", new { @class = "label" })
                                <div class="select">
                                                                        @Html.DropDownList("ddlSupplierType", new List<SelectListItem>

                                                                        {
                                                                            new SelectListItem{ Text="Customer", Value = "2" },
                                                                            new SelectListItem{ Text="Supplier", Value = "1" },
                                                                         }, new { @class = "form-control", @id = "ddlSupplierType" })
                                    <i></i>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                @Html.Label("SupplierCode", " ", new { @class = "label", @id = "labelSupplierCode" })
                                <div class="input">
                                    @Html.TextBox("txtSupplierCode", "", new { @class = "form-control", @id = "txtSupplierCode" })
                                    @Html.Hidden("hdnSupplierIdHeader", new { @id = "hdnSupplierIdHeader" })
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                @Html.Label(" ", new { @class = "label" })
                                <div class="clearfix">
                                    @Html.Label(" ", new { @class = "label-bold", @id = "lblName" })
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="row">

                        <div class="col-sm-3">
                            <div class="form-group">
                                @Html.Label("AsnNo", new { @class = "label" })
                                <div class="input">
                                    @Html.TextBox("txtAsnNo", "", new { @class = "form-control", @id = "txtAsnNo" })
                                </div>

                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                @Html.Label("InvoiceNo", new { @class = "label" })
                                <div class="input">
                                    @Html.TextBox("txtInvoiceNo", "", new { @class = "form-control", @id = "txtInvoiceNo" })
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                @Html.Label("PoNo", new { @class = "label" })
                                <div class="input">
                                    @Html.TextBox("txtPoNo", "", new { @class = "form-control", @id = "txtPoNo" })
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab-pane">
                    <div class="row">
                        <div class="col-sm-12">
                            <div>
                                <table id="dtAsnList"></table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab-pane">
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(m => m.GrnNo, new { @class = "label" })
                                <div class="system-label">
                                    @Html.LabelFor(m => m.GrnNo, "<System Generated>")
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.Partial("DateTimePicker", new DateTimePicker() { DefaultDate = Model.GrnDateTime.ConvertToDateTime(), FieldName = "GrnDateTime", FieldCaption = DataAnnotationHelper.GetDisplayName(Model, m => m.GrnDateTime), IsDateOnly = false, IsRequired = true, AllowFutureDate = false, AllowPastDate = false, MinDate = DateTime.Now.Date, MaxDate = DateTime.Now.Date.AddDays(1) })
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(m => m.AsnNo, new { @class = "label" })
                                <div class="clearfix">
                                </div>
                                @Html.LabelFor(m => m.AsnNo, " ", new { @id = "lblAsnNo", @class = "label-bold" })
                                @Html.HiddenFor(m => m.AsnNo, new { @id = "hdnAsnNo" })
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(m => m.SupplierCode, new { @class = "label" })
                                <div class="clearfix">
                                </div>
                                    @Html.LabelFor(m => m.SupplierCode, " ", new { @id = "lblSupplierCode", @class = "label-bold" })
                                    @Html.HiddenFor(m => m.SupplierId, new { @id = "hdnSupplierId" })
                                      @Html.HiddenFor(m => m.SupplierType, new { @id = "hdnSupplierType" })
                                    @Html.HiddenFor(m => m.SupplierCode, new { @id = "hdnSupplierCode" })
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(m => m.PoNo, new { @class = "label" })
                                <div class="clearfix">
                                    @Html.LabelFor(m => m.PoNo, " ", new { @id = "lblPoNo", @class = "label-bold" })
                                    @Html.HiddenFor(m => m.PoNo, new { @id = "hdnPoNo" })
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(m => m.PoDate, new { @class = "label" })
                                <div class="clearfix">
                                    @Html.LabelFor(m => m.PoDate, " ", new { @id = "lblPoDate", @class = "label-bold" })
                                    @Html.HiddenFor(m => m.PoDate, new { @id = "hdnPoDate" })
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(m => m.InvoiceNo, new { @class = "label" })
                                <div class="clearfix">
                                    @Html.TextBoxFor(model => model.InvoiceNo, new { @class = "form-control" })
                                </div>
                                @Html.ValidationMessageFor(model => model.InvoiceNo)
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                <div class="clearfix">
                                    @Html.Partial("DateTimePicker", new DateTimePicker() { DefaultDate = Model.InvoiceDate, FieldName = "InvoiceDate", FieldCaption = DataAnnotationHelper.GetDisplayName(Model, m => m.InvoiceDate), IsRequired = true, AllowFutureDate = true, AllowPastDate = true, SetBlank = true })
                                </div>
                                @Html.ValidationMessageFor(model => model.InvoiceDate)
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(model => model.GrnTypeId, new { @class = "label" })
                                <div class="select">
                                    @Html.DropDownListFor(model => model.GrnTypeId, new SelectList(ViewBag.GrnTypeList, "Value", "Name"), "Select Grn Type", new { @class = "form-control", @id = "ddlGrnTypeId" })
                                    <i></i>
                                </div>
                                @Html.ValidationMessageFor(model => model.GrnTypeId)
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(model => model.GatepassInId, new { @class = "label" })
                                <div class="select">
                                    @Html.DropDownListFor(model => model.GatepassInId, new SelectList(ViewBag.GatePassNoList, "Value", "Name"), "Select Gate Pass Number", new { @class = "form-control", @id = "ddlGatepassInNo" })
                                    <i></i>
                                </div>
                                @Html.ValidationMessageFor(model => model.GatepassInId)
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(m => m.DockNumber, new { @class = "label" })
                                <div class="clearfix">
                                    @Html.LabelFor(m => m.DockNumber, " ", new { @id = "lblDockNumber", @class = "label-bold" })
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(m => m.LoadNo, new { @class = "label" })
                                <div class="clearfix">
                                    @Html.TextBoxFor(model => model.LoadNo, new { @class = "form-control" })
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-12">
                            <div>
                                <table id="dtProductList"></table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
</div>



