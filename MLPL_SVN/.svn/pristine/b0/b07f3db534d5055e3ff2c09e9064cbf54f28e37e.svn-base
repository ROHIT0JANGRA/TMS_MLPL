using System;

namespace CodeLock.Models
{
  public class EnRouteExpenses
  {
    public EnRouteExpenses()
    {
      this.ChargeCode = (byte) 0;
      this.ChargeName = "";
      this.SpentAmount = new Decimal(0);
      this.BillNo = "";
      this.BillDate = DateTime.Now;
      this.StanderdAmount = new Decimal(0);
      this.Remarks = "";
    }

    public long TripsheetId { get; set; }

    public byte ChargeCode { get; set; }

    public string ChargeName { get; set; }

    public Decimal SpentAmount { get; set; }

    public string BillNo { get; set; }

    public DateTime BillDate { get; set; }

    public Decimal StanderdAmount { get; set; }

    public string Remarks { get; set; }
  }
}
