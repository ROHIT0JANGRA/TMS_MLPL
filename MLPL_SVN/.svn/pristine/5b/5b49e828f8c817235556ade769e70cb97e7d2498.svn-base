@model IEnumerable<CodeLock.Models.TaxProEwayTripSheetApiResponse>
@{
    ViewBag.Title = "eWay Consolidate Bill List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="widget-wrap material-table-widget">
    <table id="dtCustomer">
        <thead>
            <tr>
                <th>
                    TripSheet No.
                </th>
                <th>
                    Place
                </th>
                <th>
                    Vehicle No.
                </th>
                <th>
                    Document No.
                </th>
                <th>
                    Document Date
                </th>
                <th>
                    User Gstin
                </th>

                <th>
                    Entered Date
                </th>

                <th class="td-center" data-sort-ignore="true" data-hide="phone">Action</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.tripSheetNo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.fromPlace)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.vehicleNo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.transDocNo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.transDocDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.userGstin)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.enteredDate)
                    </td>
                    <td class="td-center">
                        <div class="btn-toolbar" role="toolbar">
                            <div class="btn-group" role="group">
                                <a class="btn btn-default btn-sm m-user-edit" href="@Url.Action("DownloadConsolidateeWayBillPdf", new { tripNo = item.tripSheetNo })"><span class="fa fa-file"></span></a>
                            </div>
                        </div>
                    </td>

                </tr>
            }
        </tbody>
    </table>
</div>

<script>
    $(document).ready(function () {
        SetPageLoad('Consolidate eWay Bill', 'List', 'filter', '', '');
        LoadDataTable('dtCustomer', true, true, true);
    });
</script>

