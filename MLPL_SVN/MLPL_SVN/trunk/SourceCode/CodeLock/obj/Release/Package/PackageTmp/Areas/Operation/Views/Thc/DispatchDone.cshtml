@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="widget-wrap" style="height:200px">
    <div class="alert alert-success"><h5><label id="lblMessage">Dispatch Done Successfully</label></h5></div>
    <div class="col-sm-3">
        <h4>Dispatch No</h4>
    </div>
    <div class="col-sm-4">
        <div class="form-group">
            @Html.Label(" ", new { @id = "lblDocumentNo", @for = "DocumentNo", @class = "label-bold" })
            @Html.Hidden("hdnDocumentId")
        </div>
    </div>
    @*<div class="col-sm-2">
        <button onclick="return ViewReport();" class="btn btn-info">
            <i class="fa fa-eye"></i>
        </button>
    </div>*@
</div>

<script>
    $(document).ready(function () {
        SetPageLoad('Dispatch', 'Done', '', 'Go To Dispatch', '@Url.Action("Dispatch")');
        lblDocumentNo = $('#lblDocumentNo');
        hdnDocumentId = $('#hdnDocumentId');

        lblDocumentNo.html(GetUrlValue()["documentNo"]);
        hdnDocumentId.val(GetUrlValue()["documentId"]);
    });
    function ViewReport() {
        @*return ShowViewPrint('@ModuleHelper.Unloading', hdnDocumentId.val());*@
        return false;
    }
</script>