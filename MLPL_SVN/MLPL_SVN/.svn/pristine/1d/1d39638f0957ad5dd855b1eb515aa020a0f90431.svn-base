using System;
using System.ComponentModel.DataAnnotations;

namespace CodeLock.Models
{
  public class MasterRouteDetail
  {
    public short RouteId { get; set; }

    public short LocationId { get; set; }

    [Display(Name = "Dist (In Km)")]
    public Decimal Distance { get; set; }

    [Display(Name = "Transition Time(Hours)")]
    public byte TransitTimeHour { get; set; }

    [Display(Name = "Transition Time(Min)")]
    public byte TransitTimeMin { get; set; }

    [Display(Name = "Stoppage Time(Hours)")]
    public byte StoppageTimeHour { get; set; }

    [Display(Name = "Stoppage Time(Min)")]
    public byte StoppageTimeMin { get; set; }

    public string LocationCode { get; set; }

    public byte RouteIndex { get; set; }
  }
}
