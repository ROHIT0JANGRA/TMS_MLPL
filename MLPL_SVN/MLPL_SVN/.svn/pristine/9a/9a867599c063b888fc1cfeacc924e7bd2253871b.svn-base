using System;

namespace CodeLock.Models
{
    public class FuelSlipDetail
    {
        public FuelSlipDetail()
        {
            this.TripsheetId = 0L;
            this.FuelSlipType = "";
            this.SlipNo = "";
            this.FuelVendorId = (short)0;
            this.FuelVendorCode = "";
            this.FuelVendorName = "";
            this.Quantity = new Decimal(0);
            this.Rate = new Decimal(0);
            this.Amount = new Decimal(0);
            this.IsChecked = false;
            this.CancelReason = "";
        }

        public long TripsheetId { get; set; }

        public string FuelSlipType { get; set; }

        public string SlipNo { get; set; }

        public short FuelVendorId { get; set; }

        public string FuelVendorCode { get; set; }

        public string FuelVendorName { get; set; }

        public Decimal Quantity { get; set; }

        public Decimal Rate { get; set; }

        public Decimal Amount { get; set; }

        public DateTime? FuelSlipDate { get; set; }
        public bool IsChecked { get; set; }
        public DateTime? CancelDate { get; set; }
        public string CancelReason { get; set; }
    }
}
