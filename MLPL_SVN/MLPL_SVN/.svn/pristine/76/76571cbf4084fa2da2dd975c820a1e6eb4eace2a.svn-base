@{
    ViewBag.Title = "InsertDone";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="widget-wrap" style="height:200px">
    <div class="alert alert-success"><h5>Manifest Genetated Successfully</h5></div>
    <div class="col-sm-3">
        <h4>Manifest No</h4>
    </div>
    <div class="col-sm-4">
        <div class="form-group">
            @Html.Label(" ", new { @id = "lblDocumentNo", @for = "DocumentNo", @class = "label-bold" })
            @Html.Hidden("hdnDocumentId")
        </div>
    </div>
    <div class="col-sm-2">
        <button onclick="return ViewReport();" class="btn btn-info">
            <i class="fa fa-eye"></i>
        </button>
    </div>
</div>
<script>
    $(document).ready(function () {
        SetPageLoad('Manifest', 'Insert Done', '', 'Go To Insert', '@Url.Action("Insert")');
        lblDocumentNo = $('#lblDocumentNo');
        hdnDocumentId = $('#hdnDocumentId');

        lblDocumentNo.html(GetUrlValue()["documentNo"]);
        hdnDocumentId.val(GetUrlValue()["documentId"]);
    });

    function ViewReport() {
        return ShowViewPrint('@ModuleHelper.Manifest',hdnDocumentId.val());
    }
</script>

